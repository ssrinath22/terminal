{
    "build": {
      "beforeBuildCommand": "npm run build",
      "beforeDevCommand": "npm run start",
      "devPath": "http://localhost:3000",
      "distDir": "../build"
    },
    "package": {
      "productName": "terminal",
      "version": "0.1.0"
    },
    "tauri": {
      "allowlist": {
        "all": false,
        "shell": {
          "all": true,
          "execute": true
        }
      },
      "bundle": {
        "active": true,
        "category": "DeveloperTool",
        "icon": [
          "icons/32x32.png",
          "icons/128x128.png",
          "icons/128x128@2x.png",
          "icons/icon.icns",
          "icons/icon.ico"
        ],
        "identifier": "com.tauri.terminal",
        "targets": "all"
      },
      "security": {
        "csp": null
      },
      "windows": [
        {
          "fullscreen": false,
          "height": 1080,
          "resizable": true,
          "title": "terminal",
          "width": 2160
        }
      ]
    }
  }
  